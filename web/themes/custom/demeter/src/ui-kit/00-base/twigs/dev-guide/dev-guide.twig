        <li><a href="pages/{{ value }}/{{ value }}.html">{{ value }}</a></li>
      {% endfor %}
    </ul>

    <h3 class="titular-maquetas">Componentes</h3>
    <ul class="lista-maquetas">
      <li>
        <a href="component-list-template.html">
          Todos los componentes
        </a>
