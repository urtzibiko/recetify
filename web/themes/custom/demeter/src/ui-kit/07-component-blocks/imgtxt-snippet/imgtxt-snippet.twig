{% extends '@component-blocks/component-block.twig' %}
{% set componentName = 'imgtxt-snippet' %}
{% block content %}
  <a href="{{ url }}" class="cb-{{ componentName }}__img">
    <img src="{{ imgsrc }}" alt="{{ alt }}"/>
  </a>
  <div class="cb-{{ componentName }}__txt">
    <a href="{{ url }}">
      {{ text }}
    </a>
  </div>
{% endblock %}
